<div class="row">
    <div class="col s12 m8 offset-m2">
        <h1 class="white">Welcome {{child.nickname}}!</h1>
        <h3 class="white center-align">Here Are All Available Missions:</h3>
    </div>
</div>
<div id="" class="row">
    <div class="col s12 m8 offset-m2">
        <ul class="collection with-header white">
            <li class="collection-header red accent-4"><h4 class="center-align">Missions Available</h4></li>
            {{#each missions_assigned}}
                <a href="#!" class="collection-item">{{> missions-card}}</a>
                {{mission_title}}
                {{mission_point_value}}
                {{mission_description}}
            {{/each}}
        </ul>
    </div>