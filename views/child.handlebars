<h1>Welcome Hero!</h1>

<div id="child-summary-area" class="row">
    <div class="col s12 m6">
        <ul class="collection with-header">
            <li class="collection-header"><h4>Missions Available</h4></li>
            {{#each missions_assigned}}
                {{> missions-card}}
            {{/each}}
        </ul>
    </div>
    <div class="col s12 m6">
        <ul class="collection with-header">
            <li class="collection-header"><h4>Rewards Earned</h4></li>
            {{#each rewards_earned}}
            {{/each}}
        </ul>
    </div>
</div>

<div class="row">
    <div class="col s6">
        <div class="mission-list">
            <!--<ul>-->
                {{#each missions_assigned}}
                    {{> missions-card}}
                    <!--<li>{{!team-child-card}}</li>-->
                {{/each}}
            <!--</ul>-->
        </div>
    </div>
    <div class="col s6">
        <div class="rewards-not-redeemed">
            {{#each rewards_to_claim}}

            {{/each}}
        </div>
    </div>
</div>
