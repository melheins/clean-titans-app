<h1>Welcome Hero!</h1>

<div id="child-summary-area" class="row">
    <div class="col s12 m6">
        <ul class="collection with-header">
            <li class="collection-header"><h4 class="center-align">Missions Available</h4></li>
            {{#each missions_assigned}}
                {{> missions-card}}
            {{/each}}
        </ul>
    </div>
    <div class="col s12 m6">
        <ul class="collection with-header">
            <li class="collection-header"><h4 class="center-align">Rewards Earned</h4></li>
            {{#each rewards_earned}}
                {{> rewards-card}}
            {{/each}}
        </ul>
    </div>
</div>